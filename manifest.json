{
	"name": "NBA stats optimizer",
	"version": "0.9.4",
	"manifest_version": 2,
	"description": "nba.com box score optimizer",
	"default_locale": "en",
	"web_accessible_resources": [
		"src/inject/assets/*"
	  ],
	"browser_action": {
	  "default_icon": "icons/icon38.png"
	  },
	"content_scripts": [
	  {
		  "matches": [
			  "https://www.nba.com/game/*"
		  ],
		  "css": [
			  "src/inject/nbacom.css"
		  ],
			"js": [
			  "src/inject/nbacom.js",
			  "src/inject/jquery-3.5.1.slim.min.js"
		  ],
		  "run_at": "document_end"
    }
  ]  
}